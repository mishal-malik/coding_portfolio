std::cout << "\nEven Number 4: \t" <<num4;
int num5= 2*5;
std::cout << "\nEven Number 5: \t" <<num5;
int num6= 2*6;
std::cout << "\nEven Number 6: \t" <<num6;
int num7= 2*7;
std::cout << "\nEven Number 7: \t" <<num7;
int num8= 2*8;
std::cout << "\nEven Number 8: \t" <<num8;
int num9= 2*9;
